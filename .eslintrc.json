{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "func-names": 0,
    "jsx-quotes": [2, "prefer-single"],
    "no-console": "off",
    "react/jsx-filename-extension": [0],
    "react/prop-types": 0
  }
}
